    "## 4. モメンタム指標"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# RSI（Relative Strength Index）\n",
    "def calculate_rsi(prices, window=14):\n",
    "    delta = prices.diff()\n",
    "    gain = (delta.where(delta > 0, 0)).rolling(window=window).mean()\n",
    "    loss = (-delta.where(delta < 0, 0)).rolling(window=window).mean()\n",
    "    rs = gain / loss\n",
    "    rsi = 100 - (100 / (1 + rs))\n",
    "    return rsi\n",
    "\n",
    "df['rsi'] = calculate_rsi(df['price'])\n",
    "\n",
    "# Stochastic Oscillator\n",
    "def calculate_stochastic(prices, window=14):\n",
    "    low_min = prices.rolling(window=window).min()\n",
    "    high_max = prices.rolling(window=window).max()\n",
    "    k_percent = 100 * (prices - low_min) / (high_max - low_min)\n",
    "    return k_percent\n",
    "\n",
    "df['stoch_k'] = calculate_stochastic(df['price'])\n",
    "df['stoch_d'] = df['stoch_k'].rolling(window=3).mean()\n",
    "\n",
    "# Williams %R\n",
    "def calculate_williams_r(prices, window=14):\n",
    "    high_max = prices.rolling(window=window).max()\n",
    "    low_min = prices.rolling(window=window).min()\n",
    "    wr = -100 * (high_max - prices) / (high_max - low_min)\n",
    "    return wr\n",
    "\n",
    "df['williams_r'] = calculate_williams_r(df['price'])\n",
    "\n",
    "# ROC（Rate of Change）\n",
    "df['roc'] = df['price'].pct_change(periods=10) * 100\n",
    "\n",
    "print(\"モメンタム指標計算完了\")"
   ]
