   "metadata": {},
   "outputs": [],
   "source": [
    "# サポート・レジスタンスレベル識別\n",
    "def find_support_resistance(prices, window=20, strength=2):\n",
    "    \"\"\"\n",
    "    サポート・レジスタンスレベルを識別\n",
    "    \"\"\"\n",
    "    highs = prices.rolling(window=window, center=True).max()\n",
    "    lows = prices.rolling(window=window, center=True).min()\n",
    "    \n",
    "    # ローカルな極値を識別\n",
    "    resistance_levels = []\n",
    "    support_levels = []\n",
    "    \n",
    "    for i in range(window, len(prices) - window):\n",
    "        if prices.iloc[i] == highs.iloc[i]:\n",
    "            # 抵抗レベル候補\n",
    "            level = prices.iloc[i]\n",
    "            count = 0\n",
    "            for j in range(max(0, i-100), min(len(prices), i+100)):\n",
    "                if abs(prices.iloc[j] - level) < level * 0.02:  # 2%の幅\n",
    "                    count += 1\n",
    "            if count >= strength:\n",
    "                resistance_levels.append((prices.index[i], level))\n",
    "        \n",
    "        if prices.iloc[i] == lows.iloc[i]:\n",
    "            # サポートレベル候補\n",
    "            level = prices.iloc[i]\n",
    "            count = 0\n",
    "            for j in range(max(0, i-100), min(len(prices), i+100)):\n",
    "                if abs(prices.iloc[j] - level) < level * 0.02:  # 2%の幅\n",
    "                    count += 1\n",
    "            if count >= strength:\n",
    "                support_levels.append((prices.index[i], level))\n",
    "    \n",
    "    return support_levels, resistance_levels\n",
    "\n",
    "# 直近1年のデータでサポート・レジスタンス分析\n",
    "recent_data = df.tail(252)\n",
    "support_levels, resistance_levels = find_support_resistance(recent_data['price'])\n",
    "\n",
    "print(f\"サポートレベル数: {len(support_levels)}\")\n",
    "print(f\"レジスタンスレベル数: {len(resistance_levels)}\")\n",
    "\n",
    "# 主要なサポート・レジスタンスレベル\n",
    "if support_levels:\n",
    "    print(\"\\n主要サポートレベル:\")\n",
    "    for date, level in support_levels[-5:]:\n",
    "        print(f\"  {date.strftime('%Y-%m-%d')}: ${level:.2f}\")\n",
    "\n",
    "if resistance_levels:\n",
    "    print(\"\\n主要レジスタンスレベル:\")\n",
    "    for date, level in resistance_levels[-5:]:\n",
    "        print(f\"  {date.strftime('%Y-%m-%d')}: ${level:.2f}\")"
   ]
