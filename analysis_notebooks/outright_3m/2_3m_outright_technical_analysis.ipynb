    "## 3. ボラティリティ分析"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# ボラティリティ指標\n",
    "df['price_range'] = df['price'].rolling(window=20).max() - df['price'].rolling(window=20).min()\n",
    "df['volatility_20d'] = df['returns'].rolling(window=20).std() * np.sqrt(252)\n",
    "df['volatility_60d'] = df['returns'].rolling(window=60).std() * np.sqrt(252)\n",
    "\n",
    "# ATR（Average True Range）の簡易版\n",
    "df['high_low_pct'] = (df['price'].rolling(window=20).max() - df['price'].rolling(window=20).min()) / df['price']\n",
    "df['atr_proxy'] = df['high_low_pct'].rolling(window=14).mean()\n",
    "\n",
    "# ボリンジャーバンド\n",
    "df['bb_middle'] = df['price'].rolling(window=20).mean()\n",
    "df['bb_std'] = df['price'].rolling(window=20).std()\n",
    "df['bb_upper'] = df['bb_middle'] + (df['bb_std'] * 2)\n",
    "df['bb_lower'] = df['bb_middle'] - (df['bb_std'] * 2)\n",
    "df['bb_position'] = (df['price'] - df['bb_lower']) / (df['bb_upper'] - df['bb_lower'])\n",
    "\n",
    "print(\"ボラティリティ指標計算完了\")"
   ]
