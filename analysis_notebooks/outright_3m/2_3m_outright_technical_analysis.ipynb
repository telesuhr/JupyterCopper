    "# テクニカル指標の統計サマリー\n",
    "recent_data = df.tail(60)  # 直近60日\n",
    "\n",
    "technical_stats = {\n",
    "    'Current Price': recent_data['price'].iloc[-1],\n",
    "    'MA20': recent_data['ma20'].iloc[-1],\n",
    "    'MA50': recent_data['ma50'].iloc[-1],\n",
    "    'MA200': recent_data['ma200'].iloc[-1],\n",
    "    'RSI': recent_data['rsi'].iloc[-1],\n",
    "    'MACD': recent_data['macd'].iloc[-1],\n",
    "    'BB Position': recent_data['bb_position'].iloc[-1],\n",
    "    '20D Volatility': recent_data['volatility_20d'].iloc[-1],\n",
    "    'Williams %R': recent_data['williams_r'].iloc[-1],\n",
    "    'Stochastic %K': recent_data['stoch_k'].iloc[-1]\n",
    "}\n",
    "\n",
    "print(\"=== 現在のテクニカル指標サマリー ===")\n",
    "for indicator, value in technical_stats.items():\n",
    "    if 'Price' in indicator or 'MA' in indicator:\n",
    "        print(f\"{indicator}: ${value:.2f}\")\n",
    "    else:\n",
    "        print(f\"{indicator}: {value:.2f}\")"
