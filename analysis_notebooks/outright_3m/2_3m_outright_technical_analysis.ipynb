    "# トレンド判定\n",
    "def determine_trend(data):\n",
    "    current_price = data['price'].iloc[-1]\n",
    "    ma20 = data['ma20'].iloc[-1]\n",
    "    ma50 = data['ma50'].iloc[-1]\n",
    "    ma200 = data['ma200'].iloc[-1]\n",
    "    rsi = data['rsi'].iloc[-1]\n",
    "    macd = data['macd'].iloc[-1]\n",
    "    \n",
    "    signals = []\n",
    "    \n",
    "    # 移動平均線によるトレンド\n",
    "    if current_price > ma20 > ma50 > ma200:\n",
    "        signals.append('強気（全MA上抜け）')\n",
    "    elif current_price > ma20 > ma50:\n",
    "        signals.append('中立-強気（短中期MA上抜け）')\n",
    "    elif current_price < ma20 < ma50 < ma200:\n",
    "        signals.append('弱気（全MA下抜け）')\n",
    "    elif current_price < ma20 < ma50:\n",
    "        signals.append('中立-弱気（短中期MA下抜け）')\n",
    "    else:\n",
    "        signals.append('中立（混合シグナル）')\n",
    "    \n",
    "    # RSIによるモメンタム\n",
    "    if rsi > 70:\n",
    "        signals.append('RSI: 過熱（売りシグナル）')\n",
    "    elif rsi < 30:\n",
    "        signals.append('RSI: 過売り（買いシグナル）')\n",
    "    else:\n",
    "        signals.append('RSI: 中立')\n",
    "    \n",
    "    # MACD\n",
    "    if macd > 0:\n",
    "        signals.append('MACD: 強気')\n",
