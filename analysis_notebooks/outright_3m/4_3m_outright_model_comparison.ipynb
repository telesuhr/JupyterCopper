   "source": [
    "## 5. モデル性能比較"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# モデル性能比較テーブル\n",
    "comparison_df = pd.DataFrame({\n",
    "    'Model': list(model_results.keys()),\n",
    "    'RMSE': [result['rmse'] for result in model_results.values()],\n",
    "    'MAE': [result['mae'] for result in model_results.values()],\n",
    "    'R2': [result['r2'] for result in model_results.values()],\n",
    "    'Direction_Accuracy': [result['direction_accuracy'] for result in model_results.values()]\n",
    "})\n",
    "\n",
    "# ランキング追加（RMSEの昇順）\n",
    "comparison_df['RMSE_Rank'] = comparison_df['RMSE'].rank()\n",
    "comparison_df['R2_Rank'] = comparison_df['R2'].rank(ascending=False)\n",
    "comparison_df['Direction_Rank'] = comparison_df['Direction_Accuracy'].rank(ascending=False)\n",
    "\n",
    "# 総合スコア（各ランクの平均）\n",
    "comparison_df['Overall_Rank'] = (comparison_df['RMSE_Rank'] + \n",
    "                                comparison_df['R2_Rank'] + \n",
    "                                comparison_df['Direction_Rank']) / 3\n",
    "\n",
    "comparison_df = comparison_df.sort_values('Overall_Rank')\n",
    "\n",
    "print(\"=== モデル性能比較 ===")\n",
    "print(comparison_df.round(4))\n",
    "\n",
    "# ベストモデル\n",
    "best_model_name = comparison_df.iloc[0]['Model']\n",
    "print(f\"\\nベストモデル: {best_model_name}\")"
   ]
