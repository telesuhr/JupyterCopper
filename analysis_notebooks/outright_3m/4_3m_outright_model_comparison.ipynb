    "# 特徴量重要度分析（Random ForestとXGBoostのみ）\n",
    "def plot_feature_importance(model_name, model, feature_names, top_n=15):\n",
    "    if hasattr(model, 'feature_importances_'):\n",
    "        importances = model.feature_importances_\n",
    "        indices = np.argsort(importances)[::-1][:top_n]\n",
    "        \n",
    "        plt.figure(figsize=(12, 8))\n",
    "        plt.title(f'{model_name} - Feature Importance (Top {top_n})')\n",
    "        plt.barh(range(top_n), importances[indices])\n",
    "        plt.yticks(range(top_n), [feature_names[i] for i in indices])\n",
    "        plt.xlabel('Importance')\n",
    "        plt.gca().invert_yaxis()\n",
    "        plt.grid(True, alpha=0.3)\n",
    "        plt.tight_layout()\n",
    "        plt.show()\n",
    "        \n",
    "        # 重要特徴量リスト\n",
    "        print(f\"\\n{model_name} - 上位{top_n}特徴量:\")\n",
    "        for i, idx in enumerate(indices):\n",
    "            print(f\"  {i+1:2d}. {feature_names[idx]}: {importances[idx]:.4f}\")\n",
    "\n",
    "# Random Forestの特徴量重要度\n",
    "if 'Random Forest' in model_results:\n",
    "    plot_feature_importance('Random Forest', \n",
    "                           model_results['Random Forest']['model'], \n",
    "                           feature_columns)\n",
    "\n",
    "# XGBoostの特徴量重要度\n",
    "if 'XGBoost' in model_results:\n",
    "    plot_feature_importance('XGBoost', \n",
    "                           model_results['XGBoost']['model'], \n",
    "                           feature_columns)"
   ]
