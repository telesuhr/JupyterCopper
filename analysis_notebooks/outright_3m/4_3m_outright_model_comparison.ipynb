    "## 7. クロスバリデーション"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 時系列クロスバリデーション\n",
    "def time_series_cross_validation(models_dict, X, y, n_splits=5):\n",
    "    \"\"\"\n",
    "    時系列データ用のクロスバリデーション\n",
    "    \"\"\"\n",
    "    tscv = TimeSeriesSplit(n_splits=n_splits)\n",
    "    cv_results = {}\n",
    "    \n",
    "    for model_name, model in models_dict.items():\n",
    "        if model_name == 'ARIMA':  # ARIMAはスキップ\n",
    "            continue\n",
    "            \n",
    "        print(f\"クロスバリデーション中: {model_name}...\")\n",
    "        \n",
    "        try:\n",
    "            if model_name == 'SVR':\n",
    "                # SVRは正規化が必要\n",
    "                X_scaled = scaler.fit_transform(X)\n",
    "                scores = -cross_val_score(model, X_scaled, y, \n",
    "                                         cv=tscv, scoring='neg_mean_squared_error')\n",
    "            else:\n",
    "                scores = -cross_val_score(model, X, y, \n",
    "                                         cv=tscv, scoring='neg_mean_squared_error')\n",
    "            \n",
    "            cv_results[model_name] = {\n",
    "                'mean_rmse': np.sqrt(scores.mean()),\n",
    "                'std_rmse': np.sqrt(scores.std()),\n",
    "                'scores': scores\n",
    "            }\n",
    "            \n",
    "            print(f\"  CV RMSE: {np.sqrt(scores.mean()):.4f} ± {np.sqrt(scores.std()):.4f}\")\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"  エラー: {e}\")\n",
    "            continue\n",
    "    \n",
    "    return cv_results\n",
    "\n",
    "# クロスバリデーション実行\n",
    "print(\"=== 時系列クロスバリデーション ===")\n",
    "cv_results = time_series_cross_validation(models, X_train, y_train)\n",
    "\n",
    "# CV結果の比較\n",
    "if cv_results:\n",
    "    cv_df = pd.DataFrame({\n",
    "        'Model': list(cv_results.keys()),\n",
    "        'CV_RMSE_Mean': [result['mean_rmse'] for result in cv_results.values()],\n",
    "        'CV_RMSE_Std': [result['std_rmse'] for result in cv_results.values()]\n",
    "    })\n",
    "    \n",
    "    cv_df = cv_df.sort_values('CV_RMSE_Mean')\n",
    "    \n",
    "    print(\"\\n=== クロスバリデーション結果 ===")\n",
    "    print(cv_df.round(4))\n",
    "\n",
    "print(\"\\nクロスバリデーション完了\")"
   ]
